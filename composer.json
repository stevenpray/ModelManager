{
    "name": "pomm-project/model-manager",
    "type": "library",
    "description":  "PHP Object Model Manager for Postgresql",
    "keywords": ["orm", "postgresql", "database", "pomm"],
    "homepage": "http://www.pomm-project.org",
    "license":  "MIT",
    "authors":  [ { "name": "GrÃ©goire HUBERT", "email": "hubert.greg@gmail.com", "homepage": "http://www.pomm-project.org" } ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stevenpray/Foundation.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-pgsql": "*",
        "league/event": "^2.1",
        "psr/log": "~1.0",
        "pomm-project/foundation": "dev-master"
    },
    "require-dev": {
        "atoum/atoum" : "dev-master"
    },
    "suggest": {
        "pomm-project/cli": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PommProject\\ModelManager\\": "sources/lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PommProject\\ModelManager\\Test\\": "sources/tests"
        }
    },
    "minimum-stability": "stable"
}
